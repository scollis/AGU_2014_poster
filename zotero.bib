@article{giangrande_precipitation_2014,
	title = {Precipitation Estimation from the {ARM} Distributed Radar Network During the {MC}3E Campaign},
	issn = {1558-8424},
	url = {http://journals.ametsoc.org/doi/abs/10.1175/JAMC-D-13-0321.1},
	doi = {10.1175/JAMC-D-13-0321.1},
	abstract = {{AbstractThis} study presents radar-based precipitation estimates collected during the two-month {DOE} {ARM} - {NASA} Midlatitude Continental Convective Clouds Experiment ({MC}3E). Emphasis is on the usefulness of radar observations from the C-band and X-band scanning {ARM} precipitation radars ({CSAPR}, {XSAPR}) for rainfall estimation products to distances within 100 km of the Lamont, Oklahoma {ARM} facility. The study utilizes a dense collection of collocated {ARM}, {NASA}-{GPM} and nearby surface Oklahoma Mesonet gauge records to evaluate radar-based hourly rainfall products and campaign optimized methods over individual gauges and for areal rainfall characterizations. Rainfall products are also evaluated against the performance of a regional {NWS} {WSR}-88D S-band dual-polarization radar product. Results indicate that the {CSAPR} system may achieve similar point and areal-gauge bias and root mean square (rms) error performance to a {WSR}-88D reference for the variety of {MC}3E deep convective events sampled. The best campaign rainfall performance was achieved when using radar relations capitalizing on estimates of the specific attenuation from the {CSAPR} system. The {XSAPRs} demonstrate limited capabilities, having modest success compared to the {WSR}-88D reference for hourly rainfall accumulations under 10 mm. All rainfall estimation methods exhibit a factor of 1.5 to 2.5 reduction in rms errors for areal accumulations over a 15 km2 {NASA} dense gauge network, with the smallest errors typically associated with dual-polarization radar methods.},
	urldate = {2014-06-13},
	journal = {J. Appl. Meteor. Climatol.},
	author = {Giangrande, Scott E. and Collis, Scott and Theisen, Adam K. and Tokay, Ali},
	month = jun,
	year = {2014},
	file = {Full Text PDF:/Users/scollis/Library/Application Support/Firefox/Profiles/312cbf3x.default/zotero/storage/VXKJBTFS/Giangrande et al. - 2014 - Precipitation Estimation from the ARM Distributed .pdf:application/pdf;Snapshot:/Users/scollis/Library/Application Support/Firefox/Profiles/312cbf3x.default/zotero/storage/H8IP623C/JAMC-D-13-0321.html:text/html}
}

