	pages = {607--613},
	file = {Full Text PDF:/Users/scollis/Library/Application Support/Firefox/Profiles/312cbf3x.default/zotero/storage/253VC5H8/Stevens and Feingold - 2009 - Untangling aerosol effects on clouds and precipita.pdf:application/pdf}
