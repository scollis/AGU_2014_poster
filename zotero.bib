@article{mather_arm_2012,
	title = {The Arm Climate Research Facility: A Review of Structure and Capabilities},
	volume = {94},
	issn = {0003-0007},
	shorttitle = {The Arm Climate Research Facility},
	url = {http://journals.ametsoc.org/doi/abs/10.1175/BAMS-D-11-00218.1},
	doi = {10.1175/BAMS-D-11-00218.1},
	abstract = {The Atmospheric Radiation Measurement ({ARM}) Climate Research Facility (www.arm.gov) provides atmospheric observations from diverse climatic regimes around the world. Because it is a U.S. Department of Energy ({DOE}) user facility, {ARM} data are freely available to anyone through the {ARM} Data Archive. With 20 years of operations, the facility recently added two mobile facilities and an aerial facility to its network of fixed-location sites. Research using {ARM} data has led to advances in areas ranging from radiative transfer to cloud microphysics. The American Recovery and Reinvestment Act of 2009 allowed {ARM} to enhance its observational capabilities with a broad array of new instruments at its fixed and mobile sites and the aerial facility. Instruments include scanning radars; water vapor, cloud/aerosol extinction, and Doppler lidars; aerosol instruments for measuring optical, physical, and chemical properties; and aircraft probes for measuring cloud and aerosol properties. Taking full advantage of these instruments will involve the development of complex data products. This work is underway but will benefit from engagement with the broader scientific community. This article describes the current status of the {ARM} research capabilities with an emphasis on developments over the past eight years since {ARM} was designated a {DOE} scientific user facility, reviews some of scientific advances made using the {ARM} Facility over the past two decades, and describes the new measurement capabilities and adaptations of the {ARM} facility to make effective use of these capabilities.},
	number = {3},
	urldate = {2014-06-12},
	journal = {Bull. Amer. Meteor. Soc.},
	author = {Mather, James H. and Voyles, Jimmy W.},
	month = aug,
	year = {2012},
	pages = {377--392},
	file = {Full Text PDF:/Users/scollis/Library/Application Support/Firefox/Profiles/312cbf3x.default/zotero/storage/RJ4ICBX4/Mather and Voyles - 2012 - The Arm Climate Research Facility A Review of Str.pdf:application/pdf;Snapshot:/Users/scollis/Library/Application Support/Firefox/Profiles/312cbf3x.default/zotero/storage/T6IJSHWN/BAMS-D-11-00218.html:text/html}
}

@misc{scott_collis_arm_2012,
	address = {Toulouse, France.},
	type = {Poster},
	title = {The {ARM} end-to-end geophysical retrieval system},
	abstract = {The {ARM} Climate Research Facility is a Department of Energy user facility with a focus on improving the representation of precipitating and non-precipitating clouds in climate models. To this end the facility has installed a network of volume scanning radars including radars at C and X band, for retrieval of geophysical parameters pertinent to precipitating systems. This presentation outlines the suite of algorithms used to progress from moment data to model-like three-dimensional geophysical parameters, including quantitative precipitation and three dimensional storm dynamics. Taking advantage of the recent {MC}3E field program, analysis has focused primarily on the Southern Great Plains radar network. However, in addition, some results from Manus Island in the tropical western Pacific and Barrow on the north slope of Alaska will be presented. As retrievals from these systems will be used to improve the cloud and climate models, a particular emphasis will be placed on verification of the retrievals with independent measurements.},
	author = {{Scott Collis} and {Scott E Giangrande} and {Kirk North} and {Nitin Bharadwaj} and {Pavlos Kollias}},
	month = jul,
	year = {2012}
}

